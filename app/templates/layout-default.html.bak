        <script type="text/javascript">

        window.onload = function(){
            document.getElementById('gsc-i-id1').placeholder = ' O que você procura? ';
            var url_string = window.location.href;
            var url = new URL(url_string);
            var data = url.searchParams.get("ac"); //pega o value
            if(data === "contraste"){
                var contrastte = localStorage.getItem("ativaContraste")==1 ? 0 : 1;
                localStorage.setItem("ativaContraste",contrastte);
            }
            if(localStorage.getItem("ativaContraste") == 1){
                var link = document.createElement( "link" );
                link.href = "{BASE_URL}templates/_assets/themes/default/css/acessibilidade.css";
                link.type = "text/css";
                link.rel = "stylesheet";
                link.media = "screen,print";

                document.getElementsByTagName( "head" )[0].appendChild( link );
            }
        };
            try {
                Typekit.load();
                window.location.reload(true);
            } catch (e) {
            }

            function exibir(chk, campo) {
                if (chk.checked) {
                    document.getElementById(campo).style.display = 'block';
                } else {
                    document.getElementById(campo).value = '';
                    document.getElementById(campo).style.display = 'none';
                }
            }

            function enviar(valor,botao) {
                var valorForm = $('.formSubmit').val();
                $('input[name='+botao+']').val(valor);
                $('form[name=' + valorForm + ']').submit();
            }

            function AbreJanela(url,largura,altura) {
                window.open(url,'pagina','status=no,scrollbars=no,left=20,top=150,width='+largura+',height='+altura);
            }

            function carregaProcesso(valor){window.location.reload(true);
                var valorForm = $('.formSubmit').val();
                $('input[name=Botao]').val('carregaProcesso');
                $('input[name=carregaProcesso]').val(valor);
                $('form[name=' + valorForm + ']').submit();
            }

            jQuery(document).ready(function($) {

                if($("input[name=TipoCnpjCpf][value=CNPJ]").is(":checked") == false && $("input[name=TipoCnpjCpf][value=CPF").is(":checked") == false){
                    $("input[name=TipoCnpjCpf][value=CPF]").click();
                }
                else{
                    if($("input[name=TipoCnpjCpf][value=CNPJ]").is(":checked") == true){
                        $("input[name=TipoCnpjCpf][value=CNPJ]").click();
                    }
                    else{
                        $("input[name=TipoCnpjCpf][value=CPF]").click();
                    }
                }


                jQuery(".reset").click(function (event) {
                    event.preventDefault();

                    jQuery(":text").each(function () {
                        jQuery(this).val("");
                    });

                    jQuery(":password").each(function () {
                        jQuery(this).val("");
                    });

        //          jQuery(":radio").each(function () {
        //              jQuery(this).prop({ checked: true })
        //          });

                    jQuery(":checkbox").each(function () {
                        jQuery(this).prop({ checked: false })
                    });

                    jQuery("select").each(function () {
                        jQuery(this).val("");
                    });
                });

                $(".telefone").mask("9999-9999");
                $(".ataMascara").mask("9999.9999/9999", {placeholder:"_"});
                $(".cnpjMascara").mask("99.999.999/9999-99");
                $(".cpfMascara").mask("999.999.999-99");
                $(".processoMascara").mask("9999/9999");
                $(".numero").keydown(function(event){
                    /* Allow backspace, delete, tab, esc e enter */
                    if ( event.keyCode == 46 || event.keyCode == 8 || event.keyCode == 9 || event.keyCode == 27 || event.keyCode == 13 ||
                        /* Allow CTRL+A */ (event.keyCode == 65 && event.ctrlKey === true) ||
                         /* Allow CTRL+C */ (event.keyCode == 67 && event.ctrlKey === true) ||
                         /* Allow CTRL+X */ (event.keyCode == 88 && event.ctrlKey === true) ||
                         /* Allow CTRL+V */ (event.keyCode == 86 && event.ctrlKey === true) ||
                         /* Allow Command+A (Mac) */ (event.keyCode == 65 && event.metaKey === true) ||
                         /* Allow Command+C (Mac) */ (event.keyCode == 67 && event.metaKey === true) ||
                         /* Allow Command+X (Mac) */ (event.keyCode == 88 && event.metaKey === true) ||
                         /* Allow Command+V (Mac) */ (event.keyCode == 86 && event.metaKey === true) ||
                         /* Allow home, end, left e right keys */ (event.keyCode >= 35 && event.keyCode <= 39)
                    ){
                        /* Boo */
                        return;
                    } else {
                        /* Stop key press */
                        if (event.shiftKey || (event.keyCode < 48 || event.keyCode > 57) && (event.keyCode < 96 || event.keyCode > 105 )) {
                            event.preventDefault();
                        }
                    }
                });

                $(".dinheiro4casas").maskMoney( { thousands:'.', decimal:',', precision:2 } );
            });

            function mascararCPF () {
                $("#cpfcnpj").mask("999.999.999-99");
            }
            function mascararCNPJ () {
                $("#cpfcnpj").mask("99.999.999/9999-99");
            }
            /**
             *
             */
            function getHashPortalCompras(inputLogin)
            {
                var retorno = null;
                $.ajax({
                    type: "POST",
                    url: "HashPortalCompras.php",
                    dataType: 'json',
                    async: false,
                    beforeSend: function(xhr){ //Observe o parâmetro na função
                        xhr.setRequestHeader('X-Login', inputLogin); //Mágica aqui
                    }
                }).success(function(data) {
                    retorno = data.emac;
                }).error(function(data) {
                    retorno = false;
                });

                return retorno;
            }
            document.getElementsById('gsc-i-id1').placeholder='Pesquisar';
            
